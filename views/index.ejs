<html>
  <body>
    <h1><%= title %></h1>
    <a href="/new">Write a new message</a>
    <ul>
      <% for( let i=0; i < messages.length; i++ ) { %>
      <li>
        <p><%= messages[i].user %></p>
        <p><%= messages[i].text %></p>
        <p><%= messages[i].added %></p>
        <button>Open message</button>
      </li>
      <% } %>
    </ul>
  </body>
</html>
